{
  "(\\d) x (\\d)": "$1 × $2",
  "(\\d) - (\\d)": "$1 − $2",
  "(\\w) >> (\\w)": "$1 ≫ $2",
  "(\\w) << (\\w)": "$1 ≪ $2",
  "(\\w) >= (\\w)": "$1 ≥ $2",
  "(\\w) =< (\\w)": "$1 ≤ $2",
  "\\+-": "±",
  "-\\+": "∓",
  "<->": "↔",
  "<=>": "⇔",
  "<-": "←",
  "->": "→",
  "<=": "⇐",
  "=>": "⇒",
  "\\(c\\)": "©",
  "\\(r\\)": "®",
  "\\(tm\\)": "™",
  "\\.{3}": "…",
  "--": "—",
  "(\\d)-(\\d)": "$1–$2"
}
